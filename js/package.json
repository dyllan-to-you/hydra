{
  "name": "hydra",
  "description": "Hydra Graphs",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Dyllan To",
    "email": "me@dyllan.to"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "ts-node": "ts-node",
    "dev": "npm run server:dev & npm run client:dev",
    "build": "npm run lint && npm run client:build && npm run server:compile",
    "start": "npm run client:build && npm run server:start",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && npm run client:typecheck",
    "client:dev": "vite --config client/vite.config.ts",
    "client:build": "vite build --config client/vite.config.ts",
    "client:preview": "vite preview --config client/vite.config.ts --port 5050",
    "client:typecheck": "vue-tsc --project client --noEmit",
    "server:test": "npm run lint && npm run compile && npm run jest",
    "server:dev": "ts-node-dev --no-notify --files --max_old_space_size=32768 server/",
    "server:start": "npm run compile && node dist/",
    "server:jest": "jest --forceExit --silent=false",
    "server:compile": "shx rm -rf dist/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "dist/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "@feathersjs/express": "^4.5.12",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/rest-client": "^4.5.12",
    "@feathersjs/socketio": "^4.5.13",
    "@feathersjs/socketio-client": "^4.5.13",
    "@feathersjs/transport-commons": "^4.5.12",
    "better-sqlite3": "^7.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "duckdb": "^0.3.2",
    "feathers-hooks-common": "^5.0.6",
    "feathers-knex": "^8.0.1",
    "feathers-pinia": "^0.29.6",
    "helmet": "^4.6.0",
    "knex": "^1.0.4",
    "pinia": "^2.0.11",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^2.4.0",
    "uplot": "^1.6.19",
    "uplot-vue": "^1.1.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.25",
    "@types/papaparse": "^5.3.2",
    "@types/plotly.js-dist-min": "^2.3.0",
    "@types/request": "^2.48.8",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^0.26.0",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  }
}
