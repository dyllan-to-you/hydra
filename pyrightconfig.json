{
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "src/experimental",
    "src/typestubs",
    ".venv"
  ],
  "ignore": [
    "src/oldstuff"
  ],
  "stubPath": "src/stubs",
  "reportTypeshedErrors": false,
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.8",
  "executionEnvironments": [
    {
      "root": "./",
      "venv": "./"
    }
  ]
}