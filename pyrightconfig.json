{
  "include": ["src"],

  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "src/experimental",
    "src/typestubs",
    ".venv"
  ],
  "ignore": ["src/oldstuff"],

  "stubPath": "src/stubs",
  "venvPath": "./.venv/",

  "reportTypeshedErrors": false,
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,

  "pythonVersion": "3.9",

  "executionEnvironments": [
    {
      "root": "./",
      "venv": "./"
    }
  ]
}
